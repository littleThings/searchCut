{
  "manifest_version": 2,
  "name": "searchCut",
  "description": "Providing keyboard shortcut for searching",
  "version": "1.0",
  "background": {
      "scripts": ["background.js"]    
  },
  "browser_action": {
      "default_icon": {                    // optional
        //"19": "images/icon19.png",           // optional
        //"38": "images/icon38.png"            // optional
      },
      "default_title": "Open Setting Page"
    },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      //"css": ["compass/stylesheets/myStyle.css"],
      "js": ["js/jquery-2.1.0.min.js","js/bootstrap.min.js","js/jquery-ui-1.10.4.custom.min.js","js/keycode.js","myScript.js"]
    }
  ],
  "permissions": ["tabs", "webNavigation","storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
